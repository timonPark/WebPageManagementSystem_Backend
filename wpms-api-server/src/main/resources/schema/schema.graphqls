type Query {
    menus: MenuResponseFormat
    shows(titleFilter: String): [Show]
}

type Mutation {
    createMenu(input: CreateMenuReqDto): CreateMenuResponseFormat
    updateMenu(input: UpdateMenuReqDto): UpdateMenuResponseFormat
    deleteMenu(input: IsUseMenuReqDto): IsUseMenuResponseFormat
    unUseMenu(input: IsUseMenuReqDto): IsUseMenuResponseFormat
}

